<?xml version="1.0" ?>
<!-- Модули -> Интернет -> Устройства и ресурсы -> Дерево -->
<S:Envelope xmlns:S="http://schemas.xmlsoap.org/soap/envelope/">
    <S:Header>
        <auth xmlns="http://ws.base.kernel.bgbilling.bitel.ru/" pswd="admin" user="admin"></auth>
    </S:Header>
    <S:Body>
        <ns5:inetDeviceUpdate xmlns:common="http://common.bitel.ru"
                              xmlns:ns5="http://service.common.api.inet.modules.bgbilling.bitel.ru/"
                              xmlns:xs="http://www.w3.org/2001/XMLSchema"
                              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <inetDevice xmlns="" deviceGroupIds="" deviceTypeId="1" entityId="0" entitySpecId="0" id="-1" ident="VLAN per user, Q-in-Q"
                        orderManagerDisabled="false" parentId="0">
                <entityAttributes></entityAttributes>
                <comment></comment>
                <config></config>
                <host></host>
                <password></password>
                <secret></secret>
                <username></username>
<invConfig>
# Режим поиска устройства
#   0 - по giaddr или IP-адресу источника
dhcp.deviceSearchMode=0

# Режим поиска сервиса на устройстве
#   2 - поиск по VLAN
dhcp.servSearchMode=2

# DHCP-опции в ответах
dhcp.option.serverIdentifier=192.168.99.254
dhcp.option.leaseTime=180

# Категории IP-ресурсов для динамических адресов
dhcp.ipCategories=2

# Посервисные категории IP-ресурсов для статических адресов
ip.resource.categoryId.2=2
ip.resource.categoryId.3=1
</invConfig>
                <invDeviceId>0</invDeviceId>
                <invDeviceTypeId>1</invDeviceTypeId>
                <invHost></invHost>
                <invIdentifier>VLAN per user, Q-in-Q</invIdentifier>
            </inetDevice>
            <updateInvDevice xmlns="">true</updateInvDevice>
        </ns5:inetDeviceUpdate>
    </S:Body>
</S:Envelope>
